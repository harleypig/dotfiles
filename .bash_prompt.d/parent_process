function __parent_process() {

  local parents

  parents=$(pstree -sA $PPID 2> /dev/null)

  if [ $? -eq 0 ]; then
    parents=$(pstree -A $PPID 2> /dev/null)
  fi

  parents=${parents/%-+*/}
  parents=${parents//bash---/}
  parents=${parents/systemd---slim---openbox---}
  parents=${parents/%---pstree/}
  parents=${parents//---/ : }
  parents=${parents//tmux/}

  if [[ -n $parents ]]; then
    echo "${text_red}${text_blink}(${parents})${text_reset} "
  fi

}
