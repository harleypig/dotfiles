function __parent_process() {

  #local parents=$(pstree -sA $PPID)
  #local parents=$(pstree -A $PPID)
  local parents

  if ! eval parents=$(pstree -sA $PPID 2> /dev/null); then
    eval parents=$(pstree -A $PPID)
  fi

  parents=${parents/%-+*/}
  parents=${parents//bash---/}
  parents=${parents/systemd---slim---openbox---}
  parents=${parents/%---pstree/}
  parents=${parents//---/ : }

  echo "${text_red}${text_blink}(${parents})${text_reset} "

}
