#!/bin/bash

echo "NOT READY!"
exit 1

declare -A linodes

populate_linodes() {
    while IFS=',' read -r id label; do
        linodes["$label"]="$id"
    done < <(lin linodes list --format id,label --text --no-headers)
}

# Make the filesystem
# Only once!
mkfs.ext4 "/dev/disk/by-id/scsi-0Linode_Volume_migratory"

mkdir /mnt/migratory
mount "/dev/disk/by-id/scsi-0Linode_Volume_migratory" "/mnt/migratory"

