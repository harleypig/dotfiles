{
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "features": {
    "ghcr.io/jpawlowski/devcontainer-features/codespaces-dotfiles:1": {
      "repository": "https://github.com/harleypig/dotfiles",
      "targetPath": "~/projects",
      "installCommand": "/bin/bash .devcontainer/setup.sh"
    }
    "ghcr.io/devcontainers/features/common-utils:2": {
      "installZsh": false,
      "installOhMyZsh": false,
      "installOhMyZshConfig": false
    },
    "ghcr.io/hspaans/devcontainer-features/ansible-lint:1": {},
    "ghcr.io/brokenpip3/devcontainers-bats/bats-libs:0": {},
    "ghcr.io/devcontainers/features/docker-in-docker:2": {},
    "ghcr.io/prulloac/devcontainer-features/pre-commit:1": {},
    "ghcr.io/devcontainers/features/github-cli:1": {},
    "ghcr.io/devcontainers/features/python:1": {
      "version": "3.11",
    },
    "ghcr.io/devcontainers-contrib/features/pipx-package": {
      "package": "aider-chat"
    },
    "ghcr.io/devcontainers/features/git": {},
  }
}
